{
    "name": "programster/leaddyno",
    "description": "An SDK for interfacing with the Leaddyno RESTful API",
    "keywords": ["leaddyno", "affiliates", "SDK"],
    "homepage": "https://github.com/programster/package-leaddyno",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "homepage": "https://blog.programster.org"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "psr/http-factory": "^1.1",
        "psr/http-client": "^1.0",
        "programster/collections": "^1.0.0",
        "ext-json": "*"
    },
    "require-dev" : {
        "guzzlehttp/guzzle": "^7.9",
        "slim/psr7": "1.6.*",
        "php-http/curl-client": "2.3.*",
        "fakerphp/faker": "^1.24",
        "programster/core-libs": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Programster\\LeadDyno\\": "src/",
            "Programster\\LeadDyno\\Exceptions\\": "src/exceptions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programster\\LeadDyno\\Testing\\": "testing/",
            "Programster\\LeadDyno\\Testing\\Tests\\": "testing/tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
